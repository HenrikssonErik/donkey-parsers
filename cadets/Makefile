define truncate_data
	sed -n '$(1),$(2)p;$(3)q' ../../../data/cadets-e3/$(6)/$(4).json | ../../libpvm-rs/build/pvm2csv - ../../../data/cadets-e3/$(6)/$(4)_$(5).zip ; \
	mkdir -p ../../../data/cadets-e3/$(6)/$(4)_$(5) ; \
	unzip ../../../data/cadets-e3/$(6)/$(4)_$(5).zip -d ../../../data/cadets-e3/$(6)/$(4)_$(5) ; \
	rm ../../../data/cadets-e3/$(6)/$(4)_$(5).zip
endef

